<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>


<StackPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.91" fx:controller="particles.ParticlesController">
   <children>
      <Canvas fx:id="canvas" height="200.0" width="200.0" />
      <Button mnemonicParsing="false" onAction="#onAdd" text="Button" StackPane.alignment="BOTTOM_LEFT">
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </Button>
      <Label fx:id="label" text="Label" />
   </children>
</StackPane>
